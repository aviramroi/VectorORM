{
  "name": "@vectororm/adapter-pinecone",
  "version": "1.0.1",
  "description": "Pinecone adapter for VectorORM",
  "author": "Aviram Roisman",
  "license": "Apache-2.0",
  "homepage": "https://github.com/aviramroi/VectorORM",
  "repository": {
    "type": "git",
    "url": "https://github.com/aviramroi/VectorORM.git",
    "directory": "packages/adapter-pinecone"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "dependencies": {
    "@vectororm/core": "*",
    "@pinecone-database/pinecone": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@vectororm/core": "^1.0.1"
  },
  "keywords": [
    "vectororm",
    "pinecone",
    "vector-database",
    "adapter"
  ]
}
